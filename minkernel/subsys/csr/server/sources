MAJORCOMP=csr
MINORCOMP=server

TARGETNAME=csrsrv
TARGETPATH=$(_OBJ_DIR)
PASS1_PUBLISH={$(O)\csrsrv.lib=$(MINWIN_LIB_PATH)\csrsrv.lib}
TARGETTYPE=DYNLINK
TARGETLIBS=$(MINWIN_LIB_PATH)\smdll.lib

INCLUDES=..\inc

USE_NTDLL=1

MSC_WARNING_LEVEL=/W3 /WX

C_DEFINES=$(C_DEFINES) -D_CSRSRV_

BACKGROUND_USE=1

SOURCES=srvinit.c   \
        srvloadr.c  \
        sbinit.c    \
        sbreqst.c   \
        sbapi.c     \
        apiinit.c   \
        apireqst.c  \
        session.c   \
        process.c   \
        csrdebug.c  \
        csrss.rc    \
        wait.c

DLLDEF=$(O)\csrsrv.def

UMTYPE=ntss
UMAPPL=csrss
UMLIBS=$(MINWIN_LIB_PATH)\csrsrv.lib $(O)\csrss.res
COFFBASE=csrss
